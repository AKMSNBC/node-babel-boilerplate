{
  "name": "node-babel-boilerplate",
  "version": "0.1.1",
  "description": "Simple node+babel+tape boilerplate",
  "license": "MIT",
  "repository": "revolunet/node-babel-boilerplate",
  "author": {
    "name": "Julien Bouquillon",
    "email": "julien@bouquillon.com",
    "url": "http://github.com/revolunet"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "readme": "node ./node_modules/node-readme/bin/node-readme.js",
    "test": "./node_modules/babel-tape-runner/bin/babel-tape-runner spec/**/*.spec.js | ./node_modules/.bin/tap-spec",
    "zuul": "./node_modules/zuul/bin/zuul -- spec/**/*.spec.js",
    "build": "babel -d ./dist ./src"
  },
  "main": "dist/index.js",
  "keywords": [
    "node",
    "babel",
    "test",
    "tape"
  ],
  "dependencies": {},
  "devDependencies": {
    "babel-eslint": "3.1.23",
    "babel-tape-runner": "1.1.0",
    "babelify": "6.1.2",
    "es6-template-strings": "1.0.0",
    "eslint": "1.0.0-rc-1",
    "eslint-config-airbnb": "0.0.6",
    "node-readme": "0.1.5",
    "tap-spec": "4.0.2",
    "tape": "4.0.0",
    "zuul": "3.2.0"
  }
}
